{
  "name": "vrcapi-client",
  "version": "0.2.0",
  "description": "Unofficial VRChat API Client library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint --ext .ts,.js --ignore-path .gitignore src/",
    "test": "jest",
    "commit": "cz",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mnao305/vrcapi-client.git"
  },
  "keywords": [
    "vrchat"
  ],
  "author": "mnao305 <mail@mnao305.com> (https://mnao305.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mnao305/vrcapi-client/issues"
  },
  "homepage": "https://github.com/mnao305/vrcapi-client#readme",
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@types/jest": "26.0.20",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "commitizen": "4.2.3",
    "cz-customizable": "6.3.0",
    "eslint": "7.21.0",
    "eslint-config-standard-with-typescript": "20.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.7",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-standard": "5.0.0",
    "husky": "5.1.3",
    "jest": "26.6.3",
    "pinst": "2.1.6",
    "standard-version": "9.1.1",
    "ts-jest": "26.5.3",
    "typescript": "4.2.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "ws": "^7.4.3"
  }
}
